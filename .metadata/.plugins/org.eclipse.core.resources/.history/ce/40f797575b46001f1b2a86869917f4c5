package helloworld;

import java.util.Random;

interface inter1{
	public void call();
	public void call1();
}
abstract class abc{
	abstract public void call2();
	
}

class calling extends abc implements inter1{

	@Override
	public void call() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void call1() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void call2() {
		// TODO Auto-generated method stub
		
	}

	
}


class Az{
	public void call() throws ArithmeticException, Exception {
		System.out.println("az call");
	}
}

class Bc extends Az{
	public void call() throws Exception{
		System.out.println("bc call");
	}
}


